{
    "audio": "pipewire",
    "bootloader": "grub_install",
    "config_version": "0.0.1",
    "debug": false,
    "desktop-environment": "qtile",
    "gfx_driver": "All open-source (default)",
    "harddrives": [
        "/dev/sda"
    ],
    "hostname": "archlinux",
    "kernels": [
        "linux"
    ],
    "keyboard-layout": "us",
    "mirror-region": {
        "Worldwide": {
            "https://mirror.rackspace.com/archlinux/$repo/os/$arch": true
        }
    },
    "mount_point": null,
    "nic": {
        "type": "NM"
    },
    "ntp": true,
    "packages": [
        "vim",
        "qutebrowser",
        "emacs",
        "alacritty"
    ],
    "profile": {
        "path": "/usr/lib/python3.10/site-packages/archinstall/profiles/desktop.py"
    },
    "script": "guided",
    "silent": false,
    "swap": true,
    "sys-encoding": "utf-8",
    "sys-language": "en_US",
    "timezone": "Europe/Paris",
    "version": "2.4.2"
}
